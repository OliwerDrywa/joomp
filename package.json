{
  "name": "tovia",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "bunx --bun vite --port 3000",
    "start": "bunx --bun vite --port 3000",
    "build": "vite build && tsc",
    "serve": "vite preview",
    "test.unit": "bun test tests/*.test.ts",
    "test.unit:watch": "bun test tests/*.test.ts --watch",
    "test.e2e": "bunx playwright test tests/*.spec.ts",
