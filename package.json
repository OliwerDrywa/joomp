    "@types/pako": "^2.0.3",
    "arktype": "^2.1.20",
    "lz-string": "^1.5.0",
    "pako": "^2.1.0",
    "solid-js": "^1.9.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.1",
    "@types/bun": "^1.2.18",
    "mitata": "^1.0.34",
    "playwright": "^1.54.1",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.13",
    "tailwindcss": "^4.1.11",
    "typescript": "^5.7.2",
