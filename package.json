    "test.e2e:headed": "bunx playwright test tests/*.spec.ts --headed",
    "benchmark.unit": "bunx playwright test benchmark/*.test.ts",
    "benchmark.e2e": "bunx playwright test benchmark/*.spec.ts"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.0.6",
